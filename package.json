{
  "name": "Rability",
  "version": "1.0.0",
  "main": "bin/server.js",
  "author": "Dr.ReMain <51071534@qq.com>",
  "license": "MIT",
  "keywords": [
    "webpack v3",
    "express",
    "react v16",
    "universal",
    "redux"
  ],
  "scripts": {
    "start": "npm-p start:prod",
    "start:prod": "cross-env NODE_ENV=production PORT=8080 node ./bin/server.js",
    "dev": "run-p dev:client dev:server",
    "dev:server": "cross-env NODE_ENV=development PORT=3000 WEBPACK_DLLS=true node ./bin/server.js",
    "dev:client": "cross-env NODE_ENV=development PORT=3000 WEBPACK_DLLS=true babel-node ./webpack/webpack.dev.server.js",
    "build": "cross-env NODE_ENV=production webpack --colors --config ./webpack/prod.conf.js",
    "build:dlls": "webpack --colors --config ./webpack/dlls.conf.js",
    "postinstall": "run-p build build:dlls",
    "precommit": "lint-staged",
    "prettier": "prettier-eslint --write --eslint-config-path .eslintrc './+(src|api)/**/*.js?(x)'",
    "lint": "eslint --fix src",
    "test": "cross-env jest src",
    "test-node": "cross-env NODE_PATH=api jest api",
    "lighthouse-report": "lighthouse http://localhost:8080 --view"
  },
  "lint-staged": {
    "+(src|api)/**/*.js?(x)": [
      "prettier-eslint --write --eslint-config-path .eslintrc",
      "eslint -c .eslintrc",
      "git add"
    ]
  },
  "dependencies": {
    "@feathersjs/authentication-client": "^1.0.2",
    "@feathersjs/feathers": "^3.1.3",
    "@feathersjs/rest-client": "^1.3.4",
    "@feathersjs/socketio-client": "^1.1.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "http-proxy": "^1.16.2",
    "import-inspector": "^2.0.0",
    "localforage": "^1.6.0",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "multireducer": "^3.1.1",
    "nprogress": "^0.2.0",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "recompose": "^0.26.0",
    "redial": "^0.5.0",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "^2.4.5",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "happypack": "^4.0.1",
    "html-webpack-plugin": "^3.0.6",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-cli": "^23.0.0-alpha.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "lighthouse": "^2.9.2",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "piping": "^1.0.0-rc.4",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.1",
    "postcss-nested": "^3.0.0",
    "postcss-reporter": "^5.0.0",
    "prettier-eslint-cli": "^4.7.1",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "3.11.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-isomorphic-tools": "^3.0.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ],
            "node": "current"
          },
          "useBuiltIns": "entry"
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports",
      "transform-decorators-legacy",
      "react-loadable/babel"
    ],
    "env": {
      "development": {
        "plugins": [
          "transform-react-jsx-source"
        ]
      }
    }
  }
}
