{
  "name": "react-ssr",
  "version": "0.1.0",
  "main": "bin/server.js",
  "author": "Dr.ReMain <51071534@qq.com>",
  "license": "MIT",
  "keywords": [
    "webpack v3",
    "express",
    "react v16",
    "universal",
    "redux"
  ],
  "scripts": {
    "start": "run-p start:prod start:api",
    "start:prod": "cross-env NODE_PATH=src NODE_ENV=production PORT=8080 node ./bin/server.js",
    "start:api": "cross-env NODE_PATH=api NODE_ENV=production node ./bin/api.js",
    "dev": "run-p dev:client dev:server dev:api",
    "dev:server": "cross-env NODE_PATH=src NODE_ENV=development PORT=3000 WEBPACK_DLLS=true node %NODE_DEBUG_OPTION% ./bin/server.js",
    "dev:api": "cross-env NODE_PATH=api NODE_ENV=development node ./bin/api.js",
    "dev:client": "cross-env NODE_PATH=src NODE_ENV=development PORT=3000 WEBPACK_DLLS=true node ./webpack/webpack.dev.server.js",
    "build": "cross-env NODE_ENV=production webpack --colors --config ./webpack/prod.conf.js",
    "build:dlls": "webpack --colors --config ./webpack/dlls.conf.js",
    "postinstall": "run-p build build:dlls",
    "prettier": "prettier-eslint --write --eslint-config-path .eslintrc \"./+(src|api)/**/*.js?(x)\"",
    "lint": "eslint --fix api src",
    "test": "cross-env NODE_PATH=src jest src",
    "lighthouse-report": "lighthouse http://127.0.0.1:8080 --view"
  },
  "dependencies": {
    "antd-mobile": "^2.2.1",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cookies": "^0.7.1",
    "cookies-js": "^1.2.3",
    "core-js": "^2.5.5",
    "cssnano": "^4.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "file-stream-rotator": "^0.2.1",
    "final-form": "^4.6.1",
    "history": "^4.7.2",
    "http-proxy": "^1.16.2",
    "import-inspector": "^2.0.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "morgan": "^1.9.0",
    "multireducer": "^3.1.1",
    "nprogress": "^0.2.0",
    "postcss-aspect-ratio-mini": "0.0.2",
    "postcss-write-svg": "^3.0.1",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.6.1",
    "qs": "^6.5.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-final-form": "^3.4.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "recompose": "^0.27.0",
    "redial": "^0.5.0",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^2.0.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-persist-cookie-storage": "^1.0.0",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.6.7",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cssnano-preset-advanced": "^4.0.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.0.1",
    "jest-cli": "^23.0.1",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "lighthouse": "^2.9.2",
    "lint-staged": "^7.0.4",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "piping": "^1.0.0-rc.4",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.1",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-hot-loader": "^4.0.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "4.9.1",
    "webpack-cli": "^3.0.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-isomorphic-tools": "^3.0.5"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "IE >= 8"
            ],
            "node": "current"
          },
          "useBuiltIns": "entry"
        }
      ],
      "stage-0"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd-mobile",
          "style": "css"
        }
      ],
      "transform-runtime",
      "transform-decorators-legacy",
      "react-loadable/babel",
      "react-hot-loader/babel"
    ],
    "env": {
      "development": {
        "plugins": [
          "transform-react-jsx-source"
        ]
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "+(src|api)/**/*.js?(x)": [
      "prettier-eslint --write --eslint-config-path .eslintrc",
      "eslint -c .eslintrc",
      "git add"
    ]
  }
}
